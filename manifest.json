{
  "manifest_version": 2,
  "name": "techclips",
  "description": "tech-clipsへクリップリクエストの送信を行う",
  "version": "1.0",
  "icons": { "16": "icons/icon16.png",
             "48": "icons/icon48.png",
            "128": "icons/icon128.png" },
  "browser_action": {
    "name": "send to clip",
    "default_icon": "icons/icon19.png"
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": ["js/jquery-2.2.3.min.js", "js/techclips.js"],
    "persistent": false
  }
}
